pipeline {
    agent any 
    stages {
        stage('checkout') {
            steps {
                checkout([$class: 'GitSCM', 
                branches: [[name: '*/main']],
                userRemoteConfigs: [[url: 'https://github.com/jabedhasan21/java-hello-world-with-maven.git']]])
            }
        }
        stage ('build') {
            steps {
                sh '''
                ls -lrt
                mvn clean install
                ls -lrt
                '''
            }
        }
    }
}