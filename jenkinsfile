pipeline {
    agent any 
    triggers{ 
        cron('H/2 * * * *')
         }
         stages {
            stage ('build') {
                steps {
                    sh '''
                    echo "amar"
                    sleep 5
                    '''
                }
            }
            stage ('test') {
                steps {
                    sh ''
                    ls
                    pwd
                    '''
                }
            }
         }
}