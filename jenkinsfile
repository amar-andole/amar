pipeline {
    agent 'none'
    stages {
        stage('build') {
           agent {
            label 'slave1'
           } 
            steps {
                sh '''
                 ls -lrt
                 pwd
                 tree
                 sleep 5
                 '''
            }
        }
        stage {
            agent {
            label 'slave2'
            }
            steps {
            sh '''
            pwd
            sleep 5
            '''
        }
        }
    }
}