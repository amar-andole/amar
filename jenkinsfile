pipeline {
    agent any
    stages {
        stage ('build') {
            steps {
                sh '''
                sleep 5
                '''
            }
        }
        stages ('test multiple') 
        parallel {
            stage('test1') {
               steps {
                 sh '''
                sleep 5
                '''
            }
        }
        stage ('test2') {
            steps {
                sh '''
                sleep 3
                '''
            }
        }
        stage ('test3') {
            steps {
                sh '''
                sleep 5
                '''
            }
        }
    }
}
}